pipeline {
	agent any
	
	stages {
		stage('Publish Data') {
			steps {
				checkout scm
				publishReports displayType: "dual", provider: json(name: "JSON Report", pattern: "etc/*.json")
				publishReports displayType: "dual", provider: xml(name: "XML Report", pattern: "etc/*.xml")
				publishReports displayType: "dual", provider: yaml(name: "YAML Report", pattern: "etc/*.yaml")
			} 
		}
	}
}
